<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
	<class name="com.model.SYSUSER0" table="SYSUSER0" >
		<id name="ID">
			<generator class="native" />
		</id>
		<property name="YHMC" />
		<property name="SYSNAME" />
		<many-to-one name="systems0" class="com.model.SYSTEMS0" cascade="none"
			fetch="select" column="SYSID" not-null="true"/>
				
	</class>
</hibernate-mapping>